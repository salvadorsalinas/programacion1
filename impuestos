#include"stdafx.h"
#include<iostream>
#include"conio.h"
using namespace std;


int main() {
	float a;
	float i;
	cout << "ingrese su sueldo: " << endl;
	cin >> a;
	if (a>=3200) {
		i = (a*11)/100;
	} else {
		i = 0;
	}
	cout << "el impuesto es: " << i << endl; 
	getch(); 
	return 0;
	
}
